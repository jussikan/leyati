{
  "name": "leyati",
  "description": "React component unit test script generator",
  "author": "Jussi Känsälä <kansalaj@gmail.com>",
  "version": "1.0.1",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "keywords": [
    "enzyme",
    "react",
    "test"
  ],
  "scripts": {
    "test": "node_modules/.bin/mocha -u bdd --reporter spec",
    "compile": "babel --presets es2015-node4 -d lib/ src/",
    "prepublish": "npm run lint-fix && BABEL_DISABLE_CACHE=1 ./node_modules/.bin/babel -s inline -d lib/ src/",
    "contbuild": "BABEL_DISABLE_CACHE=1 ./node_modules/.bin/babel -s inline -d lib/ src/ -w",
    "lint-fix": "node_modules/.bin/eslint --fix src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jussikan/leyati"
  },
  "bin": {
    "leyati": "./bin/leyati.js"
  },
  "files": [
    ".eslintignore",
    ".eslintrc",
    "bin",
    "config.js",
    "fixture",
    "lib",
    "README.md",
    "src",
    "test",
    "tpl"
  ],
  "engines": {
    "node": ">= 6.9.4",
    "npm": ">= 3.10.10"
  },
  "dependencies": {
    "babel": "^6",
    "babel-cli": "^6",
    "babel-core": "^6",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6",
    "babel-runtime": "^6",
    "babylon": "^6.16.1",
    "enzyme": "^2.3.0",
    "eshal": "^1.0.0",
    "glob": "^7.1.1",
    "jsdom": "^9.8.3",
    "jsdom-global": "^2.1.0",
    "mocha": "^3.1.0",
    "react": "^15",
    "react-addons-test-utils": "^15",
    "react-dom": "^15"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-react-intl": "^1.0.2",
    "expect": "^1.14.0",
    "json-loader": "^0.5.4",
    "mockrequire": "^0.0.6",
    "rimraf": "^2.6.1",
    "semver": "^5.3.0"
  }
}
